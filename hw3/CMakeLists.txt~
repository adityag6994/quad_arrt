cmake_minimum_required (VERSION 2.6)
project (sample_plugin)
find_package(OpenRAVE REQUIRED)
include_directories(${OpenRAVE_INCLUDE_DIRS})
link_directories(${OpenRAVE_LIBRARY_DIRS})
add_library(sample_plugin SHARED sample_plugin.cpp)
set_target_properties(sample_plugin PROPERTIES COMPILE_FLAGS "${OpenRAVE_CXX_FLAGS}" LINK_FLAGS "${OpenRAVE_LINK_FLAGS}")
target_link_libraries(sample_plugin ${OpenRAVE_LIBRARIES})
